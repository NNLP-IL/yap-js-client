  "version": "0.0.1",
  "description": "YAP - Yet Another Parser - Hebrew NLP JavaScript/TypeScript Client",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "tsc-watch --noClear -p ./tsconfig.json --onSuccess \"node ./dist/index.js\"",
    "lint": "eslint . --fix --ext .ts",
